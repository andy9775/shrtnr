{
  "name": "shrtnr",
  "version": "0.0.1",
  "description": "A url shortening service",
  "engines": {
    "node": "5.0.0",
    "npm": "3.5.3"
  },
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha  --full-trace -R spec --compilers jsx:./test/compiler.js test/**/*_spec.js",
    "start-db-dev": "pgrep postgres || pg_ctl -D /usr/local/var/postgres -l postgres.log start",
    "dev": "npm run start-db-dev && NODE_ENV=development node index.js",
    "make-dir:build": "[ ! -d build/ ] && mkdir build/ || true",
    "clean-dir:build": "[ -d build/ ] && rm -r build/** || npm run make-dir:build",
    "make-dir:dist": "[ ! -d public/dist/ ] && mkdir public/dist/ || true",
    "clean-dir:dist": "[ -d public/dist/ ] && rm -r public/dist/** || npm run make-dir:dist",
    "babel": "./node_modules/babel-cli/bin/babel.js ./src --out-dir ./build",
    "pack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean-dir:build && npm run babel && npm run clean-dir:dist && npm run pack",
    "prod": "npm run build && NODE_ENV=production node index.js",
    "start": "npm run prod"
  },
  "keywords": [
    "node",
    "express",
    "bitly",
    "short",
    "url"
  ],
  "author": "Andy Chrzaszcz <andy9775@gmail.com> (andyc.io)",
  "license": "Apache2.0",
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "chai": "^3.5.0",
    "esformatter-jsx": "^5.0.3",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "jsdom": "^9.4.0",
    "knex-cleaner": "^1.1.3",
    "mocha": "^2.5.3",
    "mock-knex": "^0.3.2",
    "react-addons-test-utils": "^15.2.0",
    "react-hot-loader": "^1.3.0",
    "react-tools": "^0.13.3",
    "react-transform-hmr": "^1.0.4",
    "sinon": "^1.17.4",
    "supertest": "^1.2.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "axios": "^0.10.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "bluebird": "^3.3.5",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.3",
    "check-types": "^6.0.0",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "hashids": "1.0.2",
    "helmet": "^1.3.0",
    "knex": "^0.11.7",
    "lodash": "^4.11.1",
    "pg": "^6.0.1",
    "pg-hstore": "^2.3.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-notification-system": "^0.2.7",
    "react-redux": "^4.4.5",
    "react-router": "^2.3.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "serve-favicon": "^2.3.0",
    "webpack": "^1.13.0",
    "winston": "^2.2.0"
  }
}
